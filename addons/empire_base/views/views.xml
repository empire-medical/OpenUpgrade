<odoo>
  <data>
    <record id="empire_base_account_move_view_tree" model="ir.ui.view">
      <field name="name">empire_base.account_move.view_tree</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_invoice_tree" />
      <field name="arch" type="xml">
        <xpath expr="//field[@name='invoice_date']" position="after">
          <field name="empire_date_invoice" string="Imperium Bill Date" />
        </xpath>
      </field>
    </record>
  </data>
</odoo>